shift + Print
	maimpick

super + Return
	alacritty

super + d
    j4-dmenu-desktop

super + {_,shift + }q
    bspc node -{c,k}

super + {_,shift + }{0-9}
    bspc {desktop -f,node -d} {0-9}

super + w
    bspc monitor -f next

super + shift + r
    . ~/.config/bspwm/bspwmrc

alt + Escape
    pkill -USR1 -x sxhkd
